+        int MAX_DIMENSION_FOR_UPLOAD = 1280;
+        boolean isLandscape = source.getWidth() > source.getHeight();
+
+        int newWidth, newHeight;
+        if (isLandscape) {
+            newWidth = MAX_DIMENSION_FOR_UPLOAD;
